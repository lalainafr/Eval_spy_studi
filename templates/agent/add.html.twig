{% extends 'base.html.twig' %}

{% block title %}Ajouter un agent{% endblock %}

{% block body %}
<h1>Ajouter un agent</h1>
<p>Vous pouvez ajouter un agent dans cette page</p>
<br>
{{ form_start(form)}}
{{ form_row(form.firstName)}}
{{ form_row(form.lastName)}}
{{ form_row(form.birthDate)}}
{{ form_row(form.idCode)}}
<input type="submit" value="Bouton" class="btn btn-info">
{{ form_end(form)}}
{% endblock %}